{
    "name": "fozzy-hosting/winvps-api-php",
    "description": "Fozzy Windows VPS APIv2 client library for PHP",
    "homepage": "https://fozzy.com",
    "license": "MIT",
    "version": "1.0",
    "authors": [
        {
            "name": "Fozzy Inc.",
            "homepage": "https://fozzy.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Fozzy\\WinVPS\\Api\\" : "lib/Api/" }
    },
    "autoload-dev": {
        "psr-4": { "Fozzy\\WinVPS\\Api\\" : "test/" }
    }
}
